{
  "manifest_version": 3,
  "name": "CG ONLINE XOX OYUNU | VenoX",
  "version": "2.0.0",
  "description": "CheatGlobal.com için çok oyunculu ve bot destekli XOX oyunu",
  "permissions": [ "storage" ],
  "host_permissions": [
    "https://cheatglobal.com/*",
    "https://xox-update.onrender.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://cheatglobal.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "CG ONLINE XOX OYUNU"
  },
  "background": {
    "service_worker": "background.js"
  }
}
